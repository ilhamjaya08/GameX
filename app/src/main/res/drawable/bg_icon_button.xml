<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/gamex_ripple">
    <item android:id="@android:id/mask">
        <shape android:shape="oval">
            <solid android:color="@color/gamex_ripple" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/gamex_surface_card" />
            <stroke
                android:width="1dp"
                android:color="@color/gamex_card_stroke" />
        </shape>
    </item>
</ripple>
